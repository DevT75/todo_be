{
    "builds": [
        {
            "src": "/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/main.py",
            "methods":["GET", "POST","PUT","DELETE","PATCH","OPTIONS","HEAD"],
            "headers":{
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, PATCH, OPTIONS, HEAD"
            }
        }
    ]
}